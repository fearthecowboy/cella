{
  "name": "cella.test",
  "version": "0.1.0",
  "description": "Cella test project",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "scripts": {
    "eslint-fix": "eslint  . --fix --ext .ts",
    "eslint": "eslint  . --ext .ts",
    "clean": "shx rm -rf dist .rush && shx echo Done",
    "build": "tsc -p .",
    "watch": "tsc -p . --watch",
    "prepare": "npm run build",
    "test-ci": "npm run build && npm run test",
    "test": "node --harmony ./node_modules/mocha/bin/mocha --config ./mocha-config.yaml --timeout 200000",
    "watch-test": "node --harmony ./node_modules/mocha/bin/mocha --config ./mocha-config.yaml --timeout 200000 --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/cella.git"
  },
  "keywords": [
    "cella"
  ],
  "author": "Microsoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Microsoft/cella/issues"
  },
  "homepage": "https://github.com/Microsoft/cella#readme",
  "readme": "https://github.com/Microsoft/cella/blob/master/readme.md",
  "devDependencies": {
    "@types/node": "14.14.22",
    "mocha": "8.3.2",
    "@types/mocha": "8.2.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint-plugin-notice": "0.9.10",
    "eslint": "^7.20.0",
    "@types/semver": "~7.3.4",
    "typescript": "next",
    "source-map-support": "~0.5.19",
    "shx": "0.3.3"
  },
  "dependencies": {
    "@microsoft/cella.core": "~0.1.0",
    "yaml": "2.0.0-5",
    "semver": "~7.3.4",
    "txtgen": "2.2.8"
  }
}
