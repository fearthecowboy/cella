{
  "name": "twisp",
  "version": "0.1.0",
  "description": "Twisp CLI",
  "main": "dist/main.js",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "scripts": {
    "eslint-fix": "eslint  . --fix --ext .ts",
    "eslint": "eslint  . --ext .ts",
    "clean": "shx rm -rf dist .rush *.log && shx echo Done",
    "build": "tsc -p .",
    "watch": "tsc -p . --watch",
    "prepare": "npm run build",
    "watch-test": "mocha dist/test --timeout 200000 --watch",
    "translate": "translate-strings ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Microsoft/twisp.git"
  },
  "keywords": [
    "twisp"
  ],
  "author": "Microsoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Microsoft/twisp/issues"
  },
  "homepage": "https://github.com/Microsoft/twisp#readme",
  "readme": "https://github.com/Microsoft/twisp/blob/master/readme.md",
  "devDependencies": {
    "@types/node": "~14.14.22",
    "@typescript-eslint/eslint-plugin": "~4.14.1",
    "@typescript-eslint/parser": "~4.14.1",
    "eslint": "~7.18.0",
    "@types/semver": "~7.3.4",
    "typescript": "4.2.0-beta",
    "shx": "0.3.3",
    "translate-strings": "1.0.0"
  },
  "dependencies": {
    "yaml": "~1.10.0",
    "semver": "~7.3.4",
    "chalk": "~4.1.0",
    "@microsoft/twisp.core": "~0.1.0"
  }
}